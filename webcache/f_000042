<script type="text/javascript">
        $(document).ready(function(){
            var url=window.location.href;
            $('.menu li a').each(function() {
                if(url==$(this).attr('href')){
                    $(this).parent().addClass('active');
                    $(this).parent().siblings().removeClass('active');
                }



            });
        })



</script>
